{
	"name": "hackernews_blacklist",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Hide uninteresting links via title, domain or user name filters.",
	"icons": {
		"128": "icons/128.png"
	},
	"background": { "scripts": [
		"lib/extstorage.js",
		"lib/background.js"
	] },
	"page_action": {
		"default_icon": "icons/19.png"
	},
	"options_page": "lib/options.html",
	"permissions": [
		"*://news.ycombinator.com/*",
		"file://*/**",
		
		"tabs"
	]
}
